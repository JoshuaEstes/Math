{
    "name": "joshuaestes/math",
    "homepage": "https://github.com/JoshuaEstes/Math",
    "description": "General Math Library",
    "keywords": ["bcmath", "gmp", "math"],
    "license": "MIT",
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/JoshuaEstes/Math/issues",
        "source": "https://github.com/JoshuaEstes/Math"
    },
    "autoload": {
        "psr-4": { "Math\\": "src/Math/"}
    },
    "autoload-dev": {
        "psr-4": { "Tests\\Math\\": "src/"}
    },
    "require": {
        "php": ">=5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.0",
        "mockery/mockery": "0.9.3"
    },
    "suggest": {
        "ext-gmp": "*",
        "ext-bcmath": "*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
